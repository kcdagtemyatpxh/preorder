{
  "$schema": "http://json.schemastore.org/template",
  "author": "triplesix0209@gmail.com",
  "classifications": [ "Service" ],
  "identity": "TripleSix.Project.Identity",
  "name": "TripleSix Templates: Identity project",
  "shortName": "triplesix.project.identity",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TripleSix.Identity",
  "preferNameDirectory": true,
  "symbols": {
    "hostname": {
      "type": "parameter",
      "replaces": "{Hostname}",
      "datatype": "text",
      "defaultValue": "sample.vn"
    },
    "connectionString": {
      "type": "parameter",
      "replaces": "{ConnectionString}",
      "datatype": "text",
      "defaultValue": ""
    },
    "clientId": {
      "type": "parameter",
      "replaces": "{ClientId}",
      "datatype": "text",
      "defaultValue": "identity"
    },
    "clientSecret": {
      "type": "parameter",
      "replaces": "{ClientSecret}",
      "datatype": "text",
      "defaultValue": "secret"
    }
  }
}